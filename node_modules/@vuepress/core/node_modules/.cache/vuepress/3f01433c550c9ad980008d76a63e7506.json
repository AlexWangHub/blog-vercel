Index: 0\r\n    }\r\n  },\r\n\r\n  created () {\r\n    this.refreshIndex()\r\n  },\r\n\r\n  watch: {\r\n    '$route' () {\r\n      this.refreshIndex()\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    refreshIndex () {\r\n      const index = resolveOpenGroupIndex(\r\n        this.$route,\r\n        this.items\r\n      )\r\n      if (index > -1) {\r\n        this.openGroupIndex = index\r\n      }\r\n    },\r\n\r\n    toggleGroup (index) {\r\n      this.openGroupIndex = index === this.openGroupIndex ? -1 : index\r\n    },\r\n\r\n    isActive (page) {\r\n      return isActive(this.$route, page.regularPath)\r\n    }\r\n  }\r\n}\r\n\r\nfunction resolveOpenGroupIndex (route, items) {\r\n  for (let i = 0; i < items.length; i++) {\r\n    const item = items[i]\r\n    if (descendantIsActive(route, item)) {\r\n      return i\r\n    }\r\n  }\r\n  return -1\r\n}\r\n\r\nfunction descendantIsActive (route, item) {\r\n  if (item.type === 'group') {\r\n    return item.children.some(child => {\r\n      if (child.type === 'group') {\r\n        return descendantIsActive(route, child)\r\n      } else {\r\n        return child.type === 'page' && isActive(route, child.path)\r\n      }\r\n    })\r\n  }\r\n  return false\r\n}\r\n"],"mappings":"AACA,OAAOA,YAAP,MAAyB,oCAAzB;AACA,OAAOC,WAAP,MAAwB,mCAAxB;AACA,SAASC,QAAT,QAAyB,SAAzB;AAEA,eAAe;EACbC,IAAI,EAAE,cADO;EAGbC,UAAU,EAAE;IAAEJ,YAAF;IAAgBC;EAAhB,CAHC;EAKbI,KAAK,EAAE,CACL,OADK,EAEL,OAFK,EAEK;EACV,cAHK,CAGU;EAHV,CALM;;EAWbC,IAAI,GAAI;IACN,OAAO;MACLC,cAAc,EAAE;IADX,CAAP;EAGD,CAfY;;EAiBbC,OAAO,GAAI;IACT,KAAKC,YAAL;EACD,CAnBY;;EAqBbC,KAAK,EAAE;IACL,WAAY;MACV,KAAKD,YAAL;IACD;;EAHI,CArBM;EA2BbE,OAAO,EAAE;IACPF,YAAY,GAAI;MACd,MAAMG,KAAK,GAAGC,qBAAqB,CACjC,KAAKC,MAD4B,EAEjC,KAAKC,KAF4B,CAAnC;;MAIA,IAAIH,KAAK,GAAG,CAAC,CAAb,EAAgB;QACd,KAAKL,cAAL,GAAsBK,KAAtB;MACD;IACF,CATM;;IAWPI,WAAW,CAAEJ,KAAF,EAAS;MAClB,KAAKL,cAAL,GAAsBK,KAAK,KAAK,KAAKL,cAAf,GAAgC,CAAC,CAAjC,GAAqCK,KAA3D;IACD,CAbM;;IAePV,QAAQ,CAAEe,IAAF,EAAQ;MACd,OAAOf,QAAQ,CAAC,KAAKY,MAAN,EAAcG,IAAI,