ce.vue\"\r\nimport { resolveSidebarItems } from '../util'\r\n\r\nexport default {\r\n  components: { Home, Page, Sidebar, Navbar, introduce },\r\n\r\n  data() {\r\n    return {\r\n      isSidebarOpen: false\r\n    }\r\n  },\r\n\r\n  computed: {\r\n    shouldShowNavbar() {\r\n      const { themeConfig } = this.$site\r\n      const { frontmatter } = this.$page\r\n      if (frontmatter.navbar === false || themeConfig.navbar === false) {\r\n        return false\r\n      }\r\n      return (\r\n        this.$title ||\r\n        themeConfig.logo ||\r\n        themeConfig.repo ||\r\n        themeConfig.nav ||\r\n        this.$themeLocaleConfig.nav\r\n      )\r\n    },\r\n\r\n    shouldShowSidebar() {\r\n      const { frontmatter } = this.$page\r\n      return (\r\n        !frontmatter.home &&\r\n        frontmatter.sidebar !== false &&\r\n        this.sidebarItems.length\r\n      )\r\n    },\r\n\r\n    sidebarItems() {\r\n      return resolveSidebarItems(\r\n        this.$page,\r\n        this.$page.regularPath,\r\n        this.$site,\r\n        this.$localePath\r\n      )\r\n    },\r\n\r\n    pageClasses() {\r\n      const userPageClass = this.$page.frontmatter.pageClass\r\n      return [\r\n        {\r\n          'no-navbar': !this.shouldShowNavbar,\r\n          'sidebar-open': this.isSidebarOpen,\r\n          'no-sidebar': !this.shouldShowSidebar\r\n        },\r\n        userPageClass\r\n      ]\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.$router.afterEach(() => {\r\n      this.isSidebarOpen = false\r\n    })\r\n  },\r\n\r\n  methods: {\r\n    toggleSidebar(to) {\r\n      this.isSidebarOpen = typeof to === 'boolean' ? to : !this.isSidebarOpen\r\n      this.$emit('toggle-sidebar', this.isSidebarOpen)\r\n    },\r\n\r\n    // side swipe\r\n    onTouchStart(e) {\r\n      this.touchStart = {\r\n        x: e.changedTouches[0].clientX,\r\n        y: e.changedTouches[0].clientY\r\n      }\r\n    },\r\n\r\n    onTouchEnd(e) {\r\n      const dx = e.changedTouches[0].clientX - this.touchStart.x\r\n      const dy = e.changedTouches[0].clientY - this.touchStart.y\r\n      if (Math.abs(dx) > Math.abs(dy) && Math.abs(dx) > 40) {\r\n        if (dx > 0 && this.touchStart.x <= 80) {\r\n          this.toggleSidebar(true)\r\n        } else {\r\n          this.toggleSidebar(false)\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n"],"mappings":"AACA,OAAOA,IAAP,MAAiB,4BAAjB;AACA,OAAOC,MAAP,MAAmB,8BAAnB;AACA,OAAOC,IAAP,MAAiB,4BAAjB;AACA,OAAOC,OAAP,MAAoB,+BAApB;AACA,OAAOC,SAAP,MAAsB,gCAAtB;AACA,SAASC,mBAAT,QAAoC,SAApC;AAEA,eAAe;EACbC,UAAU,EAAE;IAAEN,IAAF;IAAQE,IAAR;IAAcC,OAAd;IAAuBF,MAAvB;IAA+BG;EAA/B,CADC;;EAGbG,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE;IADV,CAAP;EAGD,CAPY;;EASbC,QAAQ,EAAE;IACRC,gBAAgB,GAAG;MACjB,MAAM;QAAEC;MAAF,IAAkB,KAAKC,KAA7B;MACA,MAAM;QAAEC;MAAF,IAAkB,KAAKC,KAA7B;;MACA,IAAID,WAAW,CAACE,MAAZ,KAAuB,KAAvB,IAAgCJ,WAAW,CAACI,MAAZ,KAAuB,KAA3D,EAAkE;QAChE,OAAO,KAAP;MACD;;MACD,OACE,KAAKC,MAAL,IACAL,WAAW,CAACM,IADZ,IAEAN,WAAW,CAACO,IAFZ,IAGAP,WAAW,CAACQ,GAHZ,IAIA,KAAKC,kBAAL,CAAwBD,GAL1B;IAOD,CAdO;;IAgBRE,iBAAiB,GAAG;MAClB,MAAM;QAAER;MAAF,IAAkB,KAAKC,KAA7B;MACA,OACE,CAACD,WAAW,CAACS,IAAb,IACAT,WAAW,CAACU,OAAZ,KAAwB,KADxB,IAEA,KAAKC,YAAL,CAAkBC,MAHpB;IAKD,CAvBO;;IAyBRD,YAAY,GAAG;MACb,OAAOnB,mBAAmB,CACxB,KAAKS,KADmB,EAExB,KAAKA,KAAL,CAAWY,WAFa,EAGxB,KAAKd,KAHmB,EAIxB,KAAKe,WAJmB,CAA1B;IAMD,CAhCO;;IAkCRC,WAAW,GAAG;MACZ,MAAMC,aAAa,GAAG,KAAKf,KAAL,CAAWD,WAAX,CAAuBiB,SAA7C;MACA,OAAO,CACL;QACE,aAAa,CAAC,KAAKpB,gBADrB;QAEE,gBAAgB,KAAKF,aAFvB;QAGE,cAAc,CAAC,KAAKa;MAHtB,CADK,EAMLQ,aANK,CAAP;IAQD;;EA5CO,CATG;;EAwDbE,OAAO,GAAG;IACR,KAAKC,OAAL,CAAaC,SAAb,CAAuB,MAAM;MAC3B,KAAKzB,aAAL,GAAqB,KAArB;IACD,CAFD;EAGD,CA5DY;;EA8Db0B,OAAO,EAAE;IACPC,aAAa,CAACC,EAAD,EAAK;MAChB,KAAK5B,aAAL,GAAqB,OAAO4B,EAAP,KAAc,SAAd,GAA0BA,EAA1B,GAA+B,CAAC,KAAK5B,aAA1D;MACA,KAAK6B,KAAL,CAAW,gBAAX,EAA6B,KAAK7B,aAAlC;IACD,CAJM;;IAMP;IACA8B,YAAY,CAACC,CAAD,EAAI;MACd,KAAKC,UAAL,GAAkB;QAChBC,CAAC,EAAEF,CAAC,CAACG,cAAF,CAAiB,CAAjB,EAAoBC,OADP;QAEhBC,CAAC,EAAEL,CAAC,CAACG,cAAF,CAAiB,CAAjB,E